{
  "name": "marketplace-agricole",
  "private": true,
  "version": "2.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:dev": "vite build --mode development",
    "build:force": "vite build --force",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "type-check:strict": "vue-tsc --noEmit --project tsconfig.strict.json",
    "type-check:ts-only": "node scripts/check-ts-only.cjs",
    "check-ts": "node scripts/check-typescript.js",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix",
    "lint:check": "eslint . --ext .js,.jsx,.cjs,.mjs",
    "lint:all": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@vueuse/components": "^14.2.1",
    "axios": "^1.13.4",
    "lodash": "^4.17.23",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^4.0.18",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vitest": "^4.0.18",
    "vue-tsc": "^2.2.0"
  }
}
